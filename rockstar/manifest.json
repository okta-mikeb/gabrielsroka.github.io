{
  "manifest_version": 3,
  "name": "rockstar-mod",
  "version": "0.3.7",
  "description": "rockstar adds a bunch of great features to Okta, like exporting to a CSV.",
  "omnibox": {
    "keyword": "rs"
  },
  "action": {
    "default_popup": "menu.html"
  },
  "icons": {
    "16": "rockstar_icon_16.png",
    "128": "rockstar_icon_128.png"
  },
  "host_permissions": [
    "https://*.okta.com/*",
    "https://*.oktapreview.com/*",
    "https://*.okta-emea.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "cookies",
    "identity"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.png",
        "*.jpg"
      ],
      "matches": [
        "https://*.okta.com/*",
        "https://*.oktapreview.com/*",
        "https://*.okta-emea.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.okta.com/*",
        "https://*.okta-emea.com/*",
        "https://*.oktapreview.com/*"
      ],
      "js": [
        "jquery-1.12.4.min.PATCHED.js",
        "rockstar.js"
      ],
      "css": [
        "rockstar.css"
      ],
      "run_at": "document_end"
    }
  ]
}